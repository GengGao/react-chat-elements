{
  "name": "react-chat-elements",
  "version": "11.0.0",
  "description": "Reactjs chat components",
  "author": "Avare Kodcu <abdurrahmaneker58@gmail.com>",
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/detaysoft/react-chat-elements.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "typescript",
    "chat",
    "css",
    "chat",
    "components",
    "detaysoft",
    "native"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/detaysoft/react-chat-elements/issues"
  },
  "homepage": "https://github.com/detaysoft/react-chat-elements#readme",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "emit-declarations": "tsc --declaration --emitDeclarationOnly",
    "dev": "webpack-dev-server --mode=development --config webpack.dev.js",
    "prod": "webpack --mode=production",
    "build": "webpack --progress --profile --colors --config webpack.prod.js && yarn emit-declarations",
    "test": "jest",
    "storybook": "start-storybook -p 8008",
    "type-check": "tsc --noEmit",
    "prettier": "prettier src/**/*.{ts,tsx,css} --list-different",
    "prettier-fix": "prettier 'src/**/*.{ts,tsx,css}' --write",
    "lint": "eslint --max-warnings=0 src --ext .ts,.tsx",
    "lint-fix": "eslint src --fix --ext .ts,.tsx"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "classnames": "^2.2.6",
    "moment": "^2.24.0",
    "react-icons": "^3.10.0",
    "timeago.js": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.0",
    "@babel/runtime": "^7.12.0",
    "@storybook/addon-storyshots": "^6.0.26",
    "@storybook/addons": "^6.0.26",
    "@storybook/api": "^6.0.26",
    "@storybook/react": "^3.4.12",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^25.2.1",
    "@types/react": "~16.9.49",
    "@types/react-test-renderer": "~16.9.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "css-loader": "^3.1.0",
    "jest": "^26.6.3",
    "jest-css-modules-transform": "^4.0.0",
    "mini-css-extract-plugin": "^1.3.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "ts-jest": "^26.0.0",
    "typescript": "4.0.3",
    "webpack": "^4.39.0",
    "webpack-cli": "^3.3.11"
  }
