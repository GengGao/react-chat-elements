{
  "name": "react-chat-elements",
  "version": "11.0.0",
  "description": "Reactjs chat components",
  "author": "Avare Kodcu <abdurrahmaneker58@gmail.com>",
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/detaysoft/react-chat-elements.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "typescript",
    "chat",
    "css",
    "chat",
    "components",
    "detaysoft",
    "native"
  ],
  "license": "MIT",
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "bugs": {
    "url": "https://github.com/detaysoft/react-chat-elements/issues"
  },
  "homepage": "https://github.com/detaysoft/react-chat-elements#readme",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "dev": "webpack-dev-server --mode=development --config webpack.dev.js",
    "prod": "webpack --mode=production",
    "build": "webpack --progress --profile --colors --config webpack.prod.js && tsc",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "npm run test -- --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/index.js",
    "type-check": "tsc",
    "prettier": "prettier src/**/*.{ts,tsx} --list-different",
    "prettier-fix": "prettier 'src/**/*.{ts,tsx}' --write",
    "lint": "eslint --max-warnings=0 src --ext .ts,.tsx",
    "lint-fix": "eslint src --fix --ext .ts,.tsx"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react-icons": "^3.10.0",
    "react-progress-bar.js": "^0.2.3",
    "react-spinkit": "^3.0.0",
    "timeago.js": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/runtime": "^7.10.2",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.5",
    "@types/identicon.js": "^2.3.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.8",
    "@types/react-icons": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "html-webpack-plugin": "^4.3.0",
    "identicon.js": "^2.3.3",
    "lorem-ipsum": "^2.0.3",
    "mini-css-extract-plugin": "^0.9.0",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-scripts": "^3.4.1",
    "react-test-renderer": "16.11.0",
    "typescript": "^3.9.5",
    "webpack-cli": "^3.3.11"
  }
}
